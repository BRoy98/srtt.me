    },
    "homepage": "https://github.com/BRoy98/srtt.me#readme",
    "dependencies": {
        "axios": "^0.19.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "concurrently": "^4.1.1",
        "cors": "^2.8.5",
        "eslint": "^6.0.1",
        "express": "^4.17.1",
        "global": "^4.4.0",
        "google-auth-library": "^7.0.3",
        "googleapis": "^42.0.0",
        "helmet": "^3.18.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.6.2",
